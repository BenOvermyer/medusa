{
    "name": "benovermyer/medusa",
    "authors": [
        {
            "name": "Ben Overmyer",
            "email": "manatrance@gmail.com",
            "homepage": "http://benovermyer.com",
            "role": "Architect/Front End Developer"
        }
    ],
    "description": "The web Database for the Royal Manticoran Navy.",
    "keywords": [
        "framework",
        "laravel",
        "trmn"
    ],
    "license": "MIT",
    "require": {
        "doctrine/dbal": "~2.3",
        "laravel/framework": "4.2.*",
        "barryvdh/laravel-ide-helper": "1.11.*@dev",
        "webpatser/laravel-countries": "dev-master",
        "jenssegers/mongodb": "*"
    },
    "require-dev": {
        "way/generators": "~2.0"
    },
    "autoload": {
        "classmap": [
            "app/commands",
            "app/controllers",
            "app/models",
            "app/database/migrations",
            "app/database/seeds",
            "app/tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-install-cmd": [
            "php artisan clear-compiled",
            "php artisan ide-helper:generate",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "php artisan clear-compiled",
            "php artisan ide-helper:generate",
            "php artisan optimize"
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "stable"
}
