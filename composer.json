    "description": "Project MEDUSA (Manticoran Enhanced Data Usability Service Architecture)",
    "keywords": [
        "framework",
        "laravel",
        "trmn"
    ],
    "type": "project",
    "license": "MIT",
    "require": {
        "php": ">=5.6.0",
        "ext-gd": "*",
        "ext-curl": "*",
        "ext-gmp": "*",
        "ext-imagick": "*",
        "ext-intl": "*",
        "ext-json": "*",
        "ext-mailparse": "*",
        "ext-mcrypt": "*",
        "ext-mongo": "*",
        "ext-redis": "*",
        "doctrine/dbal": "~2.3",
        "laravel/framework": "5.1.*",
        "barryvdh/laravel-ide-helper": "~1.11@dev",
        "webpatser/laravel-countries": "1.3.4",
        "jenssegers/mongodb": "~2.0",
        "mnshankar/CSV": "dev-master",
        "maatwebsite/excel": "~1.3.0",
        "simplesoftwareio/simple-qrcode": "~1",
        "intervention/image": "^2.3",
        "google/recaptcha": "~1.1",
        "nesbot/carbon": "1.17.0",
        "bshaffer/oauth2-server-httpfoundation-bridge": "^1.1",
        "league/csv": "^8.1",
        "laravelcollective/html": "5.1.*"
    },
    "require-dev": {
        "way/generators": "~2.0",
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan ide-helper:generate",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan ide-helper:generate",
            "php artisan optimize"
        ],
        "post-create-project-cmd": [
            "php -r \"copy('.env.example', '.env');\"",
            "php artisan key:generate"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    }
}